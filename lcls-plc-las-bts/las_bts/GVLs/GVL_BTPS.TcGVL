    {attribute 'pytmc' := '
        pv: LTLHN:LD
        expand: %d
    '}
    fbDestinations : ARRAY [1..GVL_BTPS_Constants.nDestinations] OF FB_DestinationDataStore;

END_VAR]]></Declaration>
